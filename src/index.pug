extends layouts/base.pug

block title
  | Vaibhav (@vrongmeal)

block content
  .landing-main
    .landing-main-heading
      .landing-main-heading-content
        .landing-main-heading-content-title Vaibhav
        .landing-main-heading-content-context
          p Software Developer
          p @vrongmeal
    .landing-main-footing
      div
        each link in nav
          .landing-main-footing-link
            a(href=link.href, target=link.target) #{link.title}
      div
        each link in contact
          .landing-main-footing-icon
            a(href=link.href, target="_blank")
              img(src=link.icon, alt=link.alt)
